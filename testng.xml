<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test  name="Regression">
  <parameter name="browsername" value="chrome"/>
    <classes>
    
      <class name="com.firebase.test.tests.AutomationScripts">
      <methods>

      <exclude name="WithoutPassword"/>
   <!--   <exclude name="ValidCredentials"/> -->
      <include name="InvalidCredentials"/>
      <exclude name="RememberMeCheckBox"/>
      <exclude name="ForgetPassword"/>
      </methods>
      </class>
      
      </classes>
  
    
  </test> <!-- Test -->
</suite> <!-- Suite -->
